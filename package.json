{
  "name": "lime-elements",
  "version": "0.0.0-development",
  "description": "Lime Elements",
  "author": "Lime Technologies",
  "license": "UNLICENSED",
  "module": "dist/esm/index.js",
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "SASS_PATH=node_modules stencil build",
    "dev": "SASS_PATH=node_modules sd concurrent \"stencil build --dev --watch\" \"stencil-dev-server\" ",
    "serve": "stencil-dev-server",
    "start": "npm run dev",
    "test": "jest",
    "test.watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "material-components-web": "^0.36.1"
  },
  "devDependencies": {
    "@stencil/core": "0.9.7",
    "@stencil/dev-server": "latest",
    "@stencil/less": "0.0.1",
    "@stencil/sass": "0.0.5",
    "@stencil/utils": "latest",
    "@types/jest": "^21.1.1",
    "jest": "^21.2.1",
    "semantic-release": "^15.6.1"
  },
  "release": {
    "branch": "master"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lundalogik/lime-elements.git"
  },
  "publishConfig": {
    "registry": "http://npm.lundalogik.com:4873/"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(tsx?|jsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "jsx"
    ]
  }
}
